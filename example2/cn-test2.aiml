<category>
<!-- match pattern -->
<pattern>*怎么样</pattern>
<!-- template for object -->
<template type='object'>
   <star/>用起来不错
   <random>
      <li>你好啊</li>
      <li>what the fuck</li>
   </random>
</template>
<!-- template for people -->
<template type='people'>
   <star/>很好啊，待人很和善
</template>
<!-- template for default -->
<template type='default'>
   不太清楚额
</template>
</category>

